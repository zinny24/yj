<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>AJAX를 이용하여 게시판 출력하기</title>
    <script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>
</head>
<body>

    <div id="table-content"></div>
    <button onclick="loadTable()">테이블 가져오기</button>

    <script>
        function loadTable() {

            $.get("/exData6", function( result ) {
                let tbl = "<table border='1'><tr><td>제목</td><td>작성자</td></tr>";

                for(let i = 0; i < result.length; ++i) {
                    tbl += "<tr>";
                            tbl += "<td>"+result[i].subject+"</td>";
                            tbl += "<td>"+result[i].writer+"</td>";
                   tbl += "</tr>";
                }
                   tbl += "</table>";

                $("#table-content").html(tbl);
            });

        }
    </script>
</body>
</html>